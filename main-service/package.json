{
  "name": "blog-nestjs",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "npx prisma generate && npx prisma db push && nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "prisma:test:reset": "dotenv -e ./.env.test -- prisma migrate reset --force && dotenv -e ./.env.test -- prisma db push",
    "test:e2e": "dotenv -e .env.test -- npx prisma db push && dotenv -e .env.test -- jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs-modules/mailer": "^1.8.1",
    "@nestjs/apollo": "^10.1.6",
    "@nestjs/bull": "^0.6.0",
    "@nestjs/common": "9.3.9",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "9.3.9",
    "@nestjs/graphql": "^10.1.6",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/passport": "^8.2.1",
    "@nestjs/platform-express": "^8.0.0",
    "@nestjs/swagger": "^5.2.1",
    "@prisma/client": "4.6.1",
    "apollo-server-express": "^3.11.1",
    "bcrypt": "^5.0.1",
    "best-string": "^1.0.0",
    "bull": "^4.8.5",
    "chalk": "4.1.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "dotenv-cli": "^6.0.0",
    "graphql": "^16.6.0",
    "multer": "1.4.5-lts.1",
    "nanoid": "^3.3.4",
    "nodemailer": "^6.7.7",
    "passport": "^0.5.2",
    "passport-jwt": "^4.0.0",
    "prisma": "4.2.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "sharp": "^0.30.4",
    "swagger-ui-express": "^4.3.0",
    "typeorm": "^0.2.44",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@jest/types": "^29.3.1",
    "@nestjs/cli": "^8.0.0",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.4.7",
    "@types/bcrypt": "^5.0.0",
    "@types/bull": "^3.15.9",
    "@types/express": "^4.17.13",
    "@types/jest": "27.0.2",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.0.0",
    "@types/sharp": "^0.30.2",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.2.5",
    "jest-mock-extended": "2.0.4",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.2.4",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.3.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "moduleNameMapper": {
      "^src/(.*)$": "<rootDir>/$1"
    },
    "verbose": true,
    "testTimeout": 1000000,
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
